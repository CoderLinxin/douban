.container{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
}

.navBar{
  display: flex;
  position: relative;
  justify-content: center;
  align-items: center;
  font-weight: bold;
  font-size: 28rpx;
}

.icon{
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
}

.back,.home{
  height: 32rpx;
  width: 32rpx;
}

.home{
  margin-left: 40rpx;
}